{
  "name": "nuxt-treez",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun nuxt build",
    "dev": "bun --bun nuxt dev",
    "generate": "bun --bun nuxt generate",
    "preview": "bun --bun nuxt preview",
    "postinstall": "bun --bun nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate",
    "db:push": "bun drizzle-kit push",
    "db:seed": "bun --bun run server/database/seeders/index.ts",
    "db:setup": "bun run db:push && bun run db:seed"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@nuxt/eslint": "0.7.4",
    "@trpc/server": "^10.45.2",
    "@vee-validate/nuxt": "^4.15.0",
    "@vee-validate/zod": "^4.15.0",
    "clsx": "2.1.1",
    "drizzle-orm": "^0.38.3",
    "eslint": "^9.17.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nanoid": "5.0.8",
    "nanoid-dictionary": "^5.0.0-beta.1",
    "nuxt": "^3.15.1",
    "superjson": "2.2.1",
    "tailwind-merge": "2.5.4",
    "trpc-nuxt": "^0.10.22",
    "ua-parser-js": "^2.0.0",
    "vue": "latest",
    "vue-router": "latest",
    "zipson": "0.2.12",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/tabler": "^1.2.14",
    "@nuxt/ui": "^2.20.0",
    "@nuxtjs/i18n": "^9.1.1",
    "@pinia/nuxt": "^0.7.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/bun": "^1.1.15",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/ua-parser-js": "^0.7.39",
    "@vueuse/nuxt": "^11.3.0",
    "drizzle-kit": "^0.30.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "nuxt-security": "^2.1.5",
    "pinia-plugin-persistedstate": "^4.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2",
    "vue-tsc": "^2.2.0"
  }
}
